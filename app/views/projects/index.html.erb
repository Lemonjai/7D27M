
  <div id="posts_wrapper" class="skinny_wrapper">

  <% @projects.each do |project| %>
    <div class="post">
      <p class="date"><%= project.created_at.strftime("%A, %B %d") %></p>
      <h2><%= link_to project.title, project %></h2>
      <hr>
    </div> 
  <% end %>

  <div class="pagination">
    <span class="previous_page disabled">
      <- Previous
    </span>
    <em class="current">1</em>
    <a href="#" rel="next">2</a>
    <a href="#" class="next_page">Next -></a>
   </div>
</div>